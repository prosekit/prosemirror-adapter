{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "composite": true,
    "noEmit": false,
    "allowJs": true,
    "outDir": "./node_modules/.cache/tsconfig/out",
    "tsBuildInfoFile": "./node_modules/.cache/tsconfig/build"
